
<%= link_to 'Sign in with Spotify', '/auth/spotify' %>

<%= form_for(@playlist) do |f| %>
 <div class="row">
    <form class="col s8">
      <div class="row">
        <div class="input-field col s4">
          <%= f.text_field :title %>
          <label for="title">Title</label>
        </div>
      </div>
     <%= f.submit %>
    </form>
  </div>
<% end %>

<% @user.playlists.each do |playlist| %>
  <%= playlist.title %>
  <% end %>